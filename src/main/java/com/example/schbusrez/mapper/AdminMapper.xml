<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.schbusrez.mapper.AdminMapper">
    <select id="findByUsernameAndPassword" resultType="com.example.schbusrez.bean.AdminBean">
        SELECT * FROM admin WHERE adminid =#{id} AND password = #{password}
